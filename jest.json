{
  "cacheDirectory": ".jest-cache",
  "automock": false,
  "bail": false,
  "testPathIgnorePatterns": [
    ".idea",
    "/node_modules"
  ],
  "collectCoverage": true,
  "verbose": false,
  "collectCoverageFrom": [
    "src/frontend/**/*.{ts,tsx}",
    "!src/frontend/reducers/index.ts",
    "!src/frontend/index.tsx",
    "!src/frontend/store.ts",
    "!node_modules/**",
    "!**/*.d.ts"
  ],
  "clearMocks": true,
  "coverageReporters": [
    "text",
    "html"
  ],
  "transform": {
    ".(ts|tsx)": "ts-jest"
  },
  "testRegex": "spec.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ]
}
